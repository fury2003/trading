bearTrap_trapBar()      => close[1] < open[1] and volume[1] < emaVol25  ? 1 : 0
bearTrap_reverseBar()   => close > open and ((close > open[1] and volume > volume[1]) or (close > bodyMiddle[1] and (volume > emaVol25 or volume > volume[1])))         ? 1 : 0

